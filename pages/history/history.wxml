<!--pages/history/history.wxml-->
<van-empty description="浏览记录暂时为空" wx:if="{{History.length==0}}" />
<view  wx:for="{{History}}"   id="{{item.newsid}}" class="newslist" wx:key="index">
  <van-card  bindtap="navigate" id="{{item.newsid}}" >
<div slot="title" class="title">{{item.title}}</div>

<view slot="desc" class="desc">{{item.inner}}"</view>

</van-card>
</view>